<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sky.Bean">
	<class name="VoiceScream" table="voicescream">
		<id name="voiceScream_id" column="voiceScream_id">
			<generator class="increment" />
		</id>
		<property name="voice_fileName" />
		
		<many-to-one name="user" column="user_id" class="User" lazy="false"/>
		
	</class>
</hibernate-mapping>